{
    "name": "bot-template",
    "version": "1.0.0",
    "description": "",
    "main": "dist/index.js",
    "scripts": {
        "register": "ts-node src/register.ts",
        "dev": "nodemon src/bot.ts",
        "postinstall": "prisma generate",
        "build": "tsc",
        "start": "pnpm register && node dist/bot.js",
        "lint": "eslint --no-error-on-unmatched-pattern --fix --max-warnings 0 src",
        "precommit": "lint-staged",
        "prepare": "husky install"
    },
    "lint-staged": {
        "./**/*.{ts,tsx}": [
            "eslint --no-error-on-unmatched-pattern --fix --max-warnings 0",
            "prettier --write"
        ]
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@types/i18n": "^0.13.4",
        "@types/node": "^18.7.13",
        "@typescript-eslint/eslint-plugin": "^5.36.1",
        "@typescript-eslint/parser": "^5.36.1",
        "eslint": "^8.23.0",
        "eslint-plugin-unused-imports": "^2.0.0",
        "husky": "^8.0.1",
        "i": "^0.3.7",
        "lint-staged": "^13.0.3",
        "nodemon": "^2.0.19",
        "npm": "^8.18.0",
        "prettier": "^2.7.1",
        "prisma": "^4.2.1",
        "ts-node": "^10.9.1",
        "typescript": "^4.8.2"
    },
    "dependencies": {
