{
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": ["@typescript-eslint", "unused-imports"],
    "extends": ["plugin:@typescript-eslint/recommended"],
    "rules": {
        "unused-imports/no-unused-imports-ts": 2
    }
}
